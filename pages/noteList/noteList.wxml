<!--pages/noteList/noteList.wxml-->
<view class="container">
   <view class="bg-box">
      <image src="/assets/img/bg.png"></image>
   </view>
   <Header id='header' bindconfirmkeyword="handleConfirmKeyword" title="{{title}}" back add/>
   <tabbar id='tabbar' bindmyevent="toggleList"/>
   <view class="danmu">
      <danmu id='danmu' bindintonote="handleIntoNote" isMore="{{isMore}}" hHeight="{{hHeight}}" tHeight="{{tHeight}}" noteList="{{noteList}}" listFlag="{{listFlag}}"/>
   </view>
</view>
<toast id="toast"></toast>
